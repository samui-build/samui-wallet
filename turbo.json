{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "TAURI_DEV_HOST",
    "VITE_ACTIVE_CLUSTER_ID",
    "VITE_API_ENDPOINT",
    "VITE_CLUSTER_DEVNET",
    "VITE_CLUSTER_DEVNET_SUBSCRIPTIONS",
    "VITE_CLUSTER_LOCALNET",
    "VITE_CLUSTER_LOCALNET_SUBSCRIPTIONS",
    "VITE_CLUSTER_MAINNET",
    "VITE_CLUSTER_MAINNET_SUBSCRIPTIONS",
    "VITE_CLUSTER_TESTNET",
    "VITE_CLUSTER_TESTNET_SUBSCRIPTIONS"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", ".output", ".wxt"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    }
  },
  "ui": "stream"
}
